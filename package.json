{
	"name": "place-webpack-preset",
	"version": "1.0.5",
	"description": "A baseline webpack configuration used in conjunction with Place Framework.",
	"main": "index.js",
	"author": "Brian Kelley",
	"config": {
		"targets": "index.js config.js src",
		"prettier": {
			"path": "@valespring/code-config/config-prettier"
		}
	},
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "npx eslint $npm_package_config_targets",
		"lint:fix": "npm run lint -- --fix",
		"prettier": "npx prettier $npm_package_config_targets --check",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix"
	},
	"prettier": "./node_modules/@valespring/code-config/prettier.config.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/valespring/place-webpack-preset.git"
	},
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.11.6",
		"@babel/preset-env": "^7.11.5",
		"@valespring/code-config": "^1.0.6",
		"autoprefixer": "^9.8.6",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^4.2.2",
		"file-loader": "^6.1.0",
		"html-critical-webpack-plugin": "^2.1.0",
		"html-webpack-harddisk-plugin": "^2.0.0",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^1.6.2",
		"postcss": "^7.0.32",
		"postcss-loader": "^3.0.0",
		"prettier-webpack-plugin": "^1.2.0",
		"sass-loader": "^10.0",
		"webpack": "^5.0.0",
		"webpack-merge": "^5.8.0"
	},
	"bugs": {
		"url": "https://github.com/valespring/place-webpack-preset/issues"
	},
	"homepage": "https://github.com/valespring/place-webpack-preset#readme",
	"browserslist": [
		"Chrome >= 45",
		"Firefox ESR",
		"Edge >= 12",
		"Explorer >= 10",
		"iOS >= 9",
		"Safari >= 9",
		"Android >= 4.4",
		"Opera >= 30",
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
